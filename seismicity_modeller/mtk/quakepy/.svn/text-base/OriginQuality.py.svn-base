from QPCore import *

class OriginQuality( QPObject ):
    """
    QuakePy: OriginQuality
    """

    # <!-- UML2Py start -->
    addElements = QPElementList( (
                    QPElement( 'associatedPhaseCount', 'associatedPhaseCount', 'element', int, 'basic' ),
                    QPElement( 'usedPhaseCount', 'usedPhaseCount', 'element', int, 'basic' ),
                    QPElement( 'associatedStationCount', 'associatedStationCount', 'element', int, 'basic' ),
                    QPElement( 'usedStationCount', 'usedStationCount', 'element', int, 'basic' ),
                    QPElement( 'depthPhaseCount', 'depthPhaseCount', 'element', int, 'basic' ),
                    QPElement( 'standardError', 'standardError', 'element', float, 'basic' ),
                    QPElement( 'azimuthalGap', 'azimuthalGap', 'element', float, 'basic' ),
                    QPElement( 'secondaryAzimuthalGap', 'secondaryAzimuthalGap', 'element', float, 'basic' ),
                    QPElement( 'minimumDistance', 'minimumDistance', 'element', float, 'basic' ),
                    QPElement( 'maximumDistance', 'maximumDistance', 'element', float, 'basic' ),
                    QPElement( 'medianDistance', 'medianDistance', 'element', float, 'basic' ),
                    QPElement( 'groundTruthLevel', 'groundTruthLevel', 'element', unicode, 'basic' )
                  ) )
    # <!-- UML2Py end -->
    
    def __init__( self, **kwargs ):
        super( OriginQuality, self ).__init__( **kwargs )

        self.elements.extend( self.addElements )
        self._initMultipleElements()

        
